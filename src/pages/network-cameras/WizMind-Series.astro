---
import Layout from '../../layouts/Layout.astro';
---

<Layout title="WizMind Series | Network Cameras">
  <!-- Product Information Section -->
  <section class="py-20 bg-gradient-to-b from-blue-50 to-white">
    <div class="container mx-auto px-4">
      <div class="max-w-6xl mx-auto">
        <h1 class="text-4xl font-bold text-gray-900 mb-8">WizMind Series</h1>
        
        <!-- Header Image Section -->
        <div class="w-full mb-12 rounded-lg overflow-hidden shadow-lg">
          <img 
            src="/src/assets/WizMind/heroimg.jpg" 
            alt="WizMind Series Surveillance"
            class="w-full h-[400px] object-cover"
          />
        </div>

        <!-- Added Product Description Section -->
        <div class="mb-12 space-y-6">
          <div class="space-y-4">
            <h2 class="text-2xl font-semibold text-gray-900">WizMind - AI-Powered Surveillance Solutions</h2>
            <p class="text-gray-700">Launched by Dahua Technology, WizMind offers industry-leading deep learning algorithms for IPC, NVR, PTZ, XVR, thermal cameras and software platforms. Our solutions provide:</p>
            <ul class="list-disc list-inside space-y-2 text-gray-700 ml-4">
              <li><span class="font-medium">Reliable:</span> Strict component selection and thorough testing ensure high product reliability</li>
              <li><span class="font-medium">Precise:</span> Accurate detection and recognition of humans and vehicles</li>
              <li><span class="font-medium">Comprehensive:</span> Multiple applications including face recognition, privacy protection, and video metadata</li>
            </ul>
          </div>
        </div>

        <!-- Product Categories Tabs -->
        <div class="mb-12">
          <div class="flex flex-wrap space-x-4 border-b border-gray-200">
            {['Panoramic Series', '8 Series', '7 Series', '5 Series', 'Special Series'].map((series, index) => (
              <button 
                class={`series-btn px-6 py-3 text-sm font-medium transition-colors duration-200 ${
                  index === 0 ? 'text-blue-600 border-b-2 border-blue-600' : 'text-gray-600 hover:text-blue-600'
                }`}
                data-series={series}
              >
                {series}
              </button>
            ))}
          </div>
        </div>

        <!-- Product Series Categories -->
        <div class="subcategory-container grid md:grid-cols-4 gap-6 mb-12">
          {['Hubble Series', 'Multi-Sensor 180° Series', 'Multi-Directional Series', 'Fisheye Series'].map((subcategory) => (
            <button 
              class="subcategory-btn px-6 py-3 text-sm font-medium text-gray-700 bg-gray-100 rounded-full hover:bg-blue-50 hover:text-blue-600 transition-colors duration-200"
              data-subcategory={subcategory}
            >
              {subcategory}
            </button>
          ))}
        </div>

        <!-- Product Grid -->
        <div class="product-grid flex flex-wrap gap-4 justify-center">
          {[
            { id: 'IPC-HDW8441X-BV-3D', name: '4MP WizMind Dual-Lens Stereo Analysis Network Camera', image: '@/assets/NetworkCameras/camera1.png' },
            { id: 'DH-IPC-HDW8341XP-3D-S2', name: '3MP WizMind Dual-Lens Network Camera', image: '@/assets/NetworkCameras/camera2.png' },
            { id: 'DH-IPC-HDW8441XP-3D', name: '4MP WizMind Dual-Lens Network Camera', image: '@/assets/NetworkCameras/camera3.png' },
            { id: 'DH-IPC-HDW8341XP-BV-3D', name: '3MP WizMind Dual-Lens Network Camera', image: '@/assets/NetworkCameras/camera4.png' }
          ].map(product => (
            <div class="flex-shrink-0 w-[300px] bg-white rounded-lg shadow-md overflow-hidden hover:shadow-lg transition-all duration-300 relative">
              <!-- Color circles -->
              <div class="absolute top-2 right-2 flex gap-1.5">
                <div class="w-2.5 h-2.5 rounded-full bg-red-500"></div>
                <div class="w-2.5 h-2.5 rounded-full bg-green-500"></div>
                <div class="w-2.5 h-2.5 rounded-full bg-blue-500"></div>
              </div>
              
              <div class="p-4">
                <div class="aspect-square bg-gray-50 rounded-lg mb-4 flex items-center justify-center">
                  <img 
                    src={product.image} 
                    alt={product.name}
                    class="object-contain h-40 w-40 hover:scale-110 transition-transform duration-500"
                  />
                </div>
                <div class="space-y-2">
                  <h3 class="text-lg font-bold text-gray-900">{product.id}</h3>
                  <p class="text-gray-600 text-sm line-clamp-3">{product.name}</p>
                  <a 
                    href={`/products/${product.id}`}
                    target="_blank"
                    rel="noopener noreferrer"
                    class="inline-flex items-center gap-2 text-sm text-blue-600 hover:text-blue-700 mt-2 group"
                  >
                    View Details
                    <svg class="w-4 h-4 transition-transform group-hover:translate-x-1" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"/>
                    </svg>
                  </a>
                </div>
              </div>
            </div>
          ))}
        </div>
      </div>
    </div>
  </section>
</Layout>

<style>
  /* Add smooth scrolling */
